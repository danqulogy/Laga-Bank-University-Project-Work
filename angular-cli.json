{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "laga-bank"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "lb",
      "mobile": false,
      "styles": [
        "../node_modules/metro-dist/css/metro.css",
        "../node_modules/metro-dist/css/metro-colors.css",
        "../node_modules/metro-dist/css/metro-icons.css",
        "../node_modules/metro-dist/css/metro-responsive.css",
        "../node_modules/metro-dist/css/metro-schemes.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/jquery/dist/jquery.dataTables.min.js",
        "../node_modules/metro-dist/js/metro.js",
        "scripts.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
